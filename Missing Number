#include<bits/stdc++.h>
using namespace std;
int main()
{
ios::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	int a[n];
	int sum = 0;
	for(int i = 1; i <= n - 1; i++)
    {
        cin >> a[i];
        sum += a[i];
    }
    int tong = 0;
    for(int i = 1; i <= n; i++)
    {
        tong += i;
    }
    cout << tong - sum;
}
